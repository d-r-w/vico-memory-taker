{
  "manifest_version": 3,
  "name": "Memory Taker",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["modal.js", "cropper.js"]
    }
  ],
  "description": "Sends memories to endpoint via context menu.",
  "permissions": [
    "contextMenus",
    "storage",
    "activeTab",
    "scripting",
    "notifications",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
